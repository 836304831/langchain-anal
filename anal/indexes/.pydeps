[pydeps]
verbose = 0
pylib = False
max_module_depth = 10

max_bacon = 10
start_color= 180

exclude_exact =
    langchain.utilities
    langchain.utils
    langchain.llms.utils

exclude =
    os
    re
    sys
    collections
    __future__
    requests
    urllib3
    jinja2
    yaml
    langchain.prompts
    langchain.base_language
    langchain.schema
    langchain.sql_database
    langchain.vectorstores
    langchain.cache
    langchain.prompts
    langchain.callbacks
    langchain.agents
    langchain.tools
    langchain.memory
    langchain.utilities
    langchain.llms
    langchain.output_parsers
    langchain.utilities
    langchain.chains
    langchain.document_loaders
    langchain.chat_models
    langchain.docstore
    langchain.retrievers
    langchain.embeddings
    langchain.graphs


